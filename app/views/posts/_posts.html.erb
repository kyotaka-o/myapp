<% posts.each do |post| %>
  <div class="post">
    <div class="post__left">
      <div class="post__left__avater">     
        <p>アバタ</p>
      </div>
      <div class="post__left__name">     
        <p>名前</p>
      </div>
    </div>
    <div class="post__right">
      <div class="post__right__title">     
        <p>タイトル</p>
      </div>
      <div class="post__right__content">     
        <p>内容</p>
      </div>
      <div class="post__right__info">     
        <p>時間</p>
      </div>
    </div>
  </div>
<% end %>